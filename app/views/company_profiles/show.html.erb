<h1>Hello <%= @company_profile.name %></h1>
<h3>Inviter un salarié : </h3>
<%= form_with model: @employee, method: "invite_employee" do |f| %>
  <%= f.text_field :email, class: "form-control" %>
  <%= f.submit "Envoyer l'invitation", class: 'btn btn-primary' %>
<% end %>

<button> <%= link_to "Se déconnecter", destroy_company_session_path, method: :delete %></button>
<button> <%= link_to "Editer mon profil", edit_company_profile_path %></button>